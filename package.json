{
  "name": "vuetify-file-browser",
  "version": "1.0.2",
  "description": "Open source file manager component for Vue.js. Requires Vuetify v2.0 or higher",
  "main": "dist/vuetify-file-browser.cjs.js",
  "browser": "dist/vuetify-file-browser.esm.js",
  "module": "dist/vuetify-file-browser.esm.js",
  "unpkg": "dist/vuetify-file-browser.min.js",
  "scripts": {
    "dev": "vue-cli-service serve demo/main.js",
    "build": "vue-cli-service build demo/main.js --dest ./dist",
    "lib": "cross-env NODE_ENV=production rollup --config rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/semeniuk/vuetify-file-browser.git"
  },
  "keywords": [
    "File",
    "Browser",
    "Vuetify",
    "Vue"
  ],
  "author": "Yurii Semeniuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/semeniuk/vuetify-file-browser/issues"
  },
  "homepage": "https://github.com/semeniuk/vuetify-file-browser#readme",
  "dependencies": {
    "@mdi/font": "^5.8.55",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "axios": "^0.21.0",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.11"
  }
}
